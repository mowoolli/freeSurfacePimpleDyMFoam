FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system/surface";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{

    eta
    {
        solver          PBiCG;//smoothSolver;
