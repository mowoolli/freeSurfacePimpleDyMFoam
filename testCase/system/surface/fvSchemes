FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system/surface";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
ddtSchemes
{
    default       Euler;
}

divSchemes
{
    div(phis,eta) Gauss linearUpwind grad(eta);
}

gradSchemes
{
    grad(eta)     cellLimited Gauss linear 1;
}

laplacianSchemes
{
}


